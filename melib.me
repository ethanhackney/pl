struct array {

        func ctor() {
                self.arr = []
        }

        func push(v) {
                self.arr.push_back(v)
        }

        func sort() {
                sort it
        }

        func at(idx) {
                vp = self.arr
                for each idx:
                        vp = vp.at(idx)
                return vp.at(idx)
        }
}
